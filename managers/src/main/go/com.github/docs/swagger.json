        "/v1": {
            "post": {
                "description": "Save a new purchases",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "purchases"
                ],
                "summary": "Save a new purchases",
                "parameters": [
                    {
                        "description": "Save purchase",
                        "name": "purchase",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.PurchaseDto"
                        }
                    },
                    {
                        "type": "string",
                        "description": "Bearer",
                        "name": "Authorization",
                        "in": "header",
                        "required": true
                    }
                ],
                "responses": {
                    "201": {},
                    "400": {},
                    "403": {}
                }
            }
        },
        "/v1/all/{status}": {
            "get": {
                "description": "Get a all orders by status",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "purchases"
                ],
                "summary": "Get a all orders by status",
                "parameters": [
                    {
                        "type": "string",
