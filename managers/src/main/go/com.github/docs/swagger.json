                        "name": "status",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.OrderDto"
                        }
                    },
                    "400": {},
                    "403": {}
                }
            }
        },
        "/v1/{orderId}/{status}": {
            "put": {
                "description": "Update order by status",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "purchases"
                ],
                "summary": "Update order by status",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Order ID",
                        "name": "orderId",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Order Status",
                        "name": "status",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {},
                    "403": {},
                    "404": {}
                }
            }
        },
        "/v1/{status}": {
            "get": {
                "description": "Get a all orders by status",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "purchases"
                ],
                "summary": "Get a all orders by status",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Order Status",
                        "name": "status",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.OrderDto"
                        }
                    },
                    "400": {},
                    "403": {}
                }
            }
        }
    },
    "definitions": {
        "dto.CustomerDto": {
            "type": "object",
            "properties": {
                "customerAddress": {
                    "type": "string",
                    "example": "Brodvey"
                },
                "customerEmail": {
                    "type": "string",
                    "example": "julvern@gmail.com"
                },
                "customerName": {
                    "type": "string",
                    "example": "Julvern"
                },
                "customerPhone": {
                    "type": "string",
                    "example": "+780466869"
                },
                "id": {
                    "type": "integer",
                    "example": 1
                },
                "purchaseId": {
                    "type": "integer",
                    "example": 11
                }
            }
        },
        "dto.OrderDto": {
            "type": "object",
            "properties": {
                "amount": {
                    "type": "number",
                    "example": 1.99
                },
                "customer": {
                    "$ref": "#/definitions/dto.CustomerDto"
                },
                "id": {
                    "type": "integer",
                    "example": 1
                },
                "product": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/dto.ProductDto"
                    }
                },
                "status": {
                    "type": "string",
                    "example": "status"
                }
            }
        },
        "dto.ProductDto": {
            "type": "object",
            "properties": {
                "description": {
                    "type": "string",
                    "example": "This is phone"
                },
                "id": {
                    "type": "integer",
                    "example": 1
                },
                "images": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "name": {
                    "type": "string",
                    "example": "Phone"
                },
                "previewImage": {
                    "type": "string",
                    "example": "image"
                },
                "price": {
                    "type": "number",
                    "example": 1.11
                },
                "quantity": {
                    "type": "integer",
                    "example": 10
                }
            }
        },
        "dto.PurchaseDto": {
            "type": "object",
            "properties": {
                "orderId": {
                    "type": "integer",
                    "example": 1
                },
                "status": {
                    "type": "string",
                    "example": "open or close"
                }
            }
        }
    }
